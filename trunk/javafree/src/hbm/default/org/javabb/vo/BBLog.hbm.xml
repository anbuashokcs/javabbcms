<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
    <class 
        name="org.javabb.vo.BBLog" 
        table="jbb_log"
    >
    
        <id
            name="logId"
            type="java.lang.Long"
            column="log_id"
        >
            <generator class="increment"/>
        </id>
    
        <property
            name="topicId"
            type="java.lang.Long"
            column="topic_id"
            length="-1"
        />
        <property
            name="userId"
            type="java.lang.Long"
            column="user_id"
            length="-1"
        />
        <property
            name="subject"
            type="java.lang.String"
            column="subject"
            length="-1"
        />
        <property
            name="obs"
            type="java.lang.String"
            column="obs"
            length="-1"
        />
        <property
            name="userName"
            type="java.lang.String"
            column="user_name"
            length="-1"
        />
        <property
            name="actionDate"
            type="java.util.Date"
            column="action_date"
            length="-1"
        />		
    </class>
</hibernate-mapping>
