<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
    <class 
        name="org.javabb.vo.Partner" 
        table="jbb_partner"
    >

        <id
            name="partnerId"
            type="java.lang.Long"
            column="partner_id"
        >
            <generator class="increment"/>
        </id>
    
        <property
            name="partnerName"
            type="java.lang.String"
            column="name"
            length="255"
        />
        <property
            name="observation"
            type="java.lang.String"
            column="observation"
            length="-1"
        />         
        <property
            name="email"
            type="java.lang.String"
            column="email"
            length="-1"
        />
           
        
    </class>
</hibernate-mapping>
