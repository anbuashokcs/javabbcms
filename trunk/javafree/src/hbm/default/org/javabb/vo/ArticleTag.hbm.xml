<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>


<class 
    name="org.javabb.vo.ArticleTag" 
    table="article_tag"
>

    <id
        name="id"
        type="java.lang.Long"
        column="id"
    >
		<generator class="increment"/>
    </id>

    <property
        name="name"
        type="java.lang.String"
        column="name"
        length="-1"
    />

  
    <!-- bi-directional one-to-many association to Post -->   
    <many-to-one
        name="topic"
        class="org.javabb.vo.Topic"
        not-null="true"
    >
        <column name="id_topic" />
    </many-to-one>



</class>
</hibernate-mapping>
