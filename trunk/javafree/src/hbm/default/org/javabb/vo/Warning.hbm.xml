<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
    <class 
        name="org.javabb.vo.Warning" 
        table="warnings"
    >

    
        <id
            name="warnId"
            type="java.lang.Long"
            column="w_id"
        >
            <generator class="increment"/>
        </id>
    
        <property
            name="warnText"
            type="java.lang.String"
            column="warning_text"
            length="255"
        />
        <property
            name="warnDate"
            type="java.util.Date"
            column="w_date"
            length="255"
        />

    
    <many-to-one
        name="post"
        class="org.javabb.vo.Post"
        not-null="true"
    >
        <column name="post_id" />
    </many-to-one>
		
    <many-to-one
        name="user"
        class="org.javabb.vo.User"
        not-null="true"
    >
        <column name="moderator_id" />
    </many-to-one>		
		
		
    </class>
</hibernate-mapping>
