<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>
    <class 
        name="org.javabb.vo.SearchLockUp" 
        table="jbb_search_lockup"
    >
    
      <cache usage="read-only"/>
		
        <id
            name="lockupId"
            type="java.lang.Long"
            column="lockup_id"
        >
            <generator class="increment"/>
        </id>
    
        <property
            name="keySearch"
            type="java.lang.String"
            column="key_search"
            length="255"
        />

		<property
            name="searchDate"
            type="java.util.Date"
            column="search_date"
            length="20"
        />
		
		<property
			name="forumId"
			type="java.lang.Long"
			column="forum_id"
			length="-1"
		/>
		
		
    </class>
</hibernate-mapping>
