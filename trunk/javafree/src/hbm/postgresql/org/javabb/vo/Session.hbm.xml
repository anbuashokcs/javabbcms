<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-2.0.dtd" >
    
<hibernate-mapping>

<class 
    name="org.javabb.vo.Session" 
    table="jbb_session"
>

    <id
        name="idS"
        type="java.lang.Long"
        column="id"
    >
		<generator class="sequence">
			<param name="sequence">SEQ_SESSION</param>
        </generator>
    </id>

    <property
        name="idUser"
        type="java.lang.Long"
        column="id_user"
        length="-1"
    />


    <property
        name="idSession"
        type="java.lang.String"
        column="id_session"
        length="-1"
    />

    <property
        name="ip"
        type="java.lang.String"
        column="ip"
        length="-1"
    />

    <property
        name="dataSession"
        type="java.util.Date"
        column="data_session"
        length="-1"
    />


</class>
</hibernate-mapping>
