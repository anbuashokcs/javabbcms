<html>
<head>
<HEAD>
<TITLE>$jbbConfig.getForumConfig().getForumName() :: #message("pms")</TITLE>

#parse("forum/themes/jb2/header.vm")
 <TR>
    <TD background="forum/themes/jb2/images/_fadetop.gif" colSpan=2 height=12></TD>
 </TR>
  <TR align=middle bgColor=#ffffff>
    <TD class=mainbody colSpan=2>
      
<table width="100%" cellspacing="1" cellpadding="3" border="0" align="center">      
  <tr>
	<td valign="top" align="center" width="100%">
	  <table height="40" cellspacing="2" cellpadding="2" border="0">
		<tr valign="middle">
		  <td><a href="inbox.jbb"><img src="forum/themes/jb2/images/msg_inbox.gif" border="0" alt='#message("inbox")' /></td>
		  <td><span class="cat"><a href="inbox.jbb">#message("inbox") &nbsp;</a></span></td>

		  <td><a href="outbox.jbb"><img src="forum/themes/jb2/images/msg_sentbox.gif" border="0" alt='#message("outbox")' /></a></td>
		  <td><span class="cat"><a href="outbox.jbb">#message("outbox")</a> &nbsp;</span></td>
		</tr>
	  </table>
	</td>

	<td align="right">
	</td>
  </tr>
</table>

<br clear="all" />
<form action="deleteSelectedOutbox.jbb" method="post">
  <table width="100%" cellspacing="2" cellpadding="2" border="0" align="center">
	<tr>
	  <td align="left" valign="middle"><a href="new.jbb"><img src="forum/images/buttons/$jbbConfig.forumConfig.buttonLang/msg_newpost.gif" border="0" /></a></td>
	  <td align="left" width="100%">&nbsp;<span class="nav"><a href="forum.jbb" class="nav">$jbbConfig.getForumConfig().getForumName() :: #message("forum_index")</a></span></td>
	</tr>
  </table>

  <table border="0" cellpadding="3" cellspacing="1" width="100%" class="forumline">
	<tr>
	  <th colspan="2" ="60%" class="thTop" nowrap="nowrap">&nbsp;#message("pm.subject")&nbsp;</th>
	  <th width="20%" class="thTop" nowrap="nowrap">&nbsp;#message("pm.to")&nbsp;</th>
	  <th width="15%" class="thTop" nowrap="nowrap">&nbsp;#message("pm.date")&nbsp;</th>
	  <th width="5%" class="thTop" nowrap="nowrap">&nbsp;#message("pm.select")&nbsp;</th>
	</tr>
#set ($row = "row1")
#foreach($pm in $lstMsgs)
	<tr>
	  <td width="2%" valign="middle" class="$row"><img src="forum/themes/jb2/images/folder.gif" width="19" height="18" /></td>
	  <td width="58%" valign="middle" class="$row"><span class="topictitle">&nbsp;<a href="readOut.jbb?pm.id=$!pm.id" class="topictitle">$!pm.topic</a></span></td>
	  <td width="20%" valign="middle" align="center" class="$row"><span class="name">&nbsp;<a href="viewprofile.jbb?u=$!pm.userFrom.id" class="name">$!pm.userTo.user</a></span></td>
	  <td width="15%" align="center" valign="middle" class="$row"><span class="postdetails">$vh.dateMinuteFormat($!{pm.data})</span></td>
	  <td width="5%" align="center" valign="middle" class="$row"><span class="postdetails">
		<input type="checkbox" name="idPm" value="$pm.id" /></span></td>
	</tr>
	#if ($row == "row1") #set ($row = "row2")
	#else #set ($row = "row1") #end
#end
	<tr>
	  <td class="cat" colspan="5" height="28" align="right"> 
		<input type="submit" name="delete" value='#message("pm.removeSelected")' class="mainoption" />
	  </td>
	</tr>
  </table>
</form>
  
#parse("forum/themes/jb2/footer.vm")